# generated by kind
global
  log /dev/log local0
  log /dev/log local1 notice
  daemon

defaults
  log global
  mode tcp
  option dontlognull
  timeout connect 5000
  timeout client 50000
  timeout server 50000

frontend etcd-lb0
  bind *:2379
  default_backend etcd
  balance leastconn

backend etcd
  server member0 etcd-member0:2379 check 
  server member1 etcd-member1:2379 check
  server member2 etcd-member2:2379 check

